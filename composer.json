{
    "name": "carousel/render",
    "type": "library",
    "description": "PHP templating engine",
    "keywords": [
        "templating",
        "engine",
        "render"
    ],
    "homepage": "https://github.com/carousel/render",
    "license": "MIT",
    "authors": [
        {
            "name": "Miroslav Trninic",
            "email": "miroslav.trninic@gmail.com",
            "homepage": "http://larabal.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Carousel\\": "src"
        },
        "files": [
            "src/helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Carousel\\RenderTest\\": "tests"
        },
        "files": [
            "src/helpers/functions.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
